{
  "name": "Unclockify",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Turn Wikipedia's shitty astronomical coordinates into decimal degrees.",
  "browser_action": {
    "default_icon": "icon2.png",
    "name": ""
  },
  "background":{
    "scripts": ["jquery-2.1.1.min.js", "background.js"],
    "matches": [ "http://*/*", "https://*/*"]
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}
